{
  "name": "aaiti-backend",
  "version": "2.0.0-rc.1",
  "description": "A.A.I.T.I v2.0.0-rc.1 Backend - Production-Ready AI Trading Interface API Server with Enterprise Architecture",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "NODE_ENV=test mocha \"tests/**/*.js\" --timeout 20000",
    "test:unit": "NODE_ENV=test mocha \"tests/unit/**/*.js\" --timeout 10000",
    "test:integration": "NODE_ENV=test mocha \"../tests/mlIntegrationTests.js\" --timeout 30000",
    "test:production": "NODE_ENV=test mocha \"../tests/productionTradingIntegration.test.js\" --timeout 30000",
    "test:sprint3": "NODE_ENV=test node tests/sprint3TestRunner.js",
    "test:all": "npm run test && npm run test:integration && npm run test:production",
    "coverage": "nyc --reporter=text --reporter=lcov --check-coverage --lines 40 --branches 30 --functions 40 npm test",
    "coverage:all": "nyc --reporter=text --reporter=lcov --check-coverage --lines 50 --branches 40 --functions 50 npm run test:all",
    "migrate": "knex migrate:latest --knexfile ./knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile ./knexfile.js",
    "migrate:test": "NODE_ENV=test knex migrate:latest --knexfile ./knexfile.js",
    "init:sprint3": "node scripts/initSprint3.js",
    "validate": "node ../validate-production-trading.js",
    "lint": "eslint . --ext .js --fix",
    "security-audit": "npm audit --audit-level moderate"
  },
  "keywords": [
    "trading",
    "ai",
    "api",
    "finance"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "apollo-server-express": "^3.13.0",
    "arima": "^0.2.5",
    "axios": "^1.11.0",
    "bayes": "^1.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.1",
    "fast-csv": "^5.0.2",
    "graphql": "^16.11.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mathjs": "^14.5.3",
    "ml-matrix": "^6.12.1",
    "ml-random-forest": "^2.1.0",
    "ml-regression": "^6.3.0",
    "ml-svm": "^2.1.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "pg-pool": "^3.10.1",
    "prom-client": "^15.1.3",
    "semver": "^7.7.2",
    "simple-statistics": "^7.8.8",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "ts-arima-forecast": "^1.0.3",
    "twilio": "^5.8.0",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "chai": "^5.3.3",
    "knex": "^3.1.0",
    "mocha": "^11.7.4",
    "nodemon": "^3.1.10",
    "nyc": "^15.1.0",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4"
  }
}
