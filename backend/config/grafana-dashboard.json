{
  "dashboard": {
    "id": null,
    "title": "AAITI Trading System Metrics",
    "tags": ["aaiti", "trading", "crypto", "ai"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "10s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{app=\"aaiti-backend\"}",
            "legendFormat": "System Status",
            "refId": "A"
          },
          {
            "expr": "aaiti_http_requests_total",
            "legendFormat": "Total Requests",
            "refId": "B"
          },
          {
            "expr": "aaiti_trades_total",
            "legendFormat": "Total Trades",
            "refId": "C"
          },
          {
            "expr": "sum(aaiti_portfolio_value_usd)",
            "legendFormat": "Total Portfolio Value",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 2,
        "title": "Trading Activity",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(aaiti_trades_total[5m])",
            "legendFormat": "Trades per second - {{symbol}} {{side}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Trades/sec",
            "min": 0
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": true,
          "current": true
        }
      },
      {
        "id": 3,
        "title": "Portfolio Value",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "aaiti_portfolio_value_usd",
            "legendFormat": "Portfolio Value - {{user_id}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Value (USD)",
            "min": 0
          }
        ],
        "xAxis": {
          "show": true
        },
        "legend": {
          "show": true,
          "values": true,
          "current": true
        }
      },
      {
        "id": 4,
        "title": "API Response Times",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(aaiti_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(aaiti_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (s)",
            "min": 0
          }
        ],
        "legend": {
          "show": true
        }
      },
      {
        "id": 5,
        "title": "ML Model Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "aaiti_ml_model_accuracy",
            "legendFormat": "Accuracy - {{model_type}} {{symbol}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Accuracy (%)",
            "min": 0,
            "max": 100
          }
        ],
        "legend": {
          "show": true
        }
      },
      {
        "id": 6,
        "title": "Database Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(aaiti_db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "aaiti_db_cache_hit_rate",
            "legendFormat": "Cache Hit Rate",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (s) / Rate (%)",
            "min": 0
          }
        ]
      },
      {
        "id": 7,
        "title": "Cache Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 20},
        "targets": [
          {
            "expr": "aaiti_cache_hit_rate",
            "legendFormat": "Hit Rate - {{cache_type}}",
            "refId": "A"
          },
          {
            "expr": "aaiti_cache_size_bytes / 1024 / 1024",
            "legendFormat": "Size MB - {{cache_type}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Hit Rate (%) / Size (MB)",
            "min": 0
          }
        ]
      },
      {
        "id": 8,
        "title": "System Resources",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 20},
        "targets": [
          {
            "expr": "aaiti_memory_usage_bytes{type=\"heapUsed\"} / 1024 / 1024",
            "legendFormat": "Memory Used (MB)",
            "refId": "A"
          },
          {
            "expr": "aaiti_cpu_usage_percent",
            "legendFormat": "CPU Usage (%)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Usage",
            "min": 0
          }
        ]
      },
      {
        "id": 9,
        "title": "External API Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(aaiti_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile - {{service}}",
            "refId": "A"
          },
          {
            "expr": "rate(aaiti_api_requests_total[5m])",
            "legendFormat": "Requests/sec - {{service}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (s) / Rate (req/s)",
            "min": 0
          }
        ]
      },
      {
        "id": 10,
        "title": "WebSocket Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "aaiti_websocket_connections",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "rate(aaiti_websocket_messages_total[5m])",
            "legendFormat": "Messages/sec - {{direction}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Connections / Messages/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 11,
        "title": "Risk Management",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "aaiti_portfolio_drawdown_percent",
            "legendFormat": "Drawdown % - {{user_id}}",
            "refId": "A"
          },
          {
            "expr": "rate(aaiti_risk_violations_total[5m])",
            "legendFormat": "Risk Violations/sec - {{rule_type}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Drawdown (%) / Violations/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 12,
        "title": "Trading Bot Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "targets": [
          {
            "expr": "aaiti_trading_bot_status",
            "legendFormat": "",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true},
              "renameByName": {
                "bot_id": "Bot ID",
                "user_id": "User ID",
                "strategy": "Strategy",
                "Value": "Status"
              }
            }
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Status"},
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "mappings",
                  "value": [
                    {
                      "options": {"0": {"color": "red", "index": 0, "text": "Inactive"}},
                      "type": "value"
                    },
                    {
                      "options": {"1": {"color": "green", "index": 1, "text": "Active"}},
                      "type": "value"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "increase(aaiti_nodejs_version_info[1h])",
          "iconColor": "rgba(255, 96, 96, 1)",
          "titleFormat": "Deployment"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{app=\"aaiti-backend\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "symbol",
          "type": "query",
          "query": "label_values(aaiti_trades_total, symbol)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    }
  }
}